{
  "name": "triangles",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite --host 0.0.0.0\" \"PORT=3001 tsx watch server/index.ts\"",
    "build": "vite build",
