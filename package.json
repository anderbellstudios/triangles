    "prop-types": "^15.8.1",
    "react-a11y-dialog": "^6.2.0",
    "redis": "^4.6.14",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@playwright/test": "^1.45.0",
    "@preact/preset-vite": "^2.8.3",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.9",
    "autoprefixer": "^10.4.19",
    "concurrently": "^7.6.0",
    "postcss": "^8.4.39",
    "postcss-aspect-ratio-polyfill": "^2.0.0",
