    "prop-types": "^15.8.1",
    "react-a11y-dialog": "^6.1.5",
    "redis": "^4.2.0",
    "socket.io": "^4.5.1",
    "socket.io-client": "^4.5.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7",
    "@playwright/test": "^1.43.1",
    "@preact/preset-vite": "^2.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.12.9",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "postcss": "^8.4.14",
    "postcss-aspect-ratio-polyfill": "^2.0.0",
    "prettier": "2.7.1",
